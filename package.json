{
  "name": "three-bas",
  "version": "2.5.1",
  "description": "THREE.js Buffer Animation System",
  "repository": "https://github.com/zadvorsky/three.bas",
  "main": "src/main.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "rollup -c",
